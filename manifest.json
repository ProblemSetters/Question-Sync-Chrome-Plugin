{
    "name": "Content Sync",
    "description": "Sync content from github directly to HRW Library",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "bg.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "/icons/hackerrank.png"
        }
    },
    "icons": {
        "48": "/icons/hackerrank.png"
    },
    "host_permissions": [
        "https://www.github.com/",
        "https://raw.githubusercontent.com/",
        "https://www.hackerrank.com/"
    ]
  }